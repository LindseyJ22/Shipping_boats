$('#' + window.job_section).find('.booking_wrapper').append("<%= j render @job_boat %>");
var count = $('#' + window.job_section).find('.booking').last().data('count')
var job_id = $('#' + window.job_section).find('.booking').last().data('job_id')
var $job_count = $('#' + job_id + "_container")
